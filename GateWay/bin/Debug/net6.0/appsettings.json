{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Spring": {
    "Application": {
      "Name": "GateWay"
    }
  },
  "Eureka": {
    "Client": {
      "ServiceUrl":  "http://eureka-server:8761/eureka/",
      "ValidateCertificates": false,
      "ShouldRegisterWithEureka": true
    }
  },
  "Instance": {
    "HostName": "gateway",
    "NonSecurePort": 80,
    "InstanceId": "GateWay"
    //"appName": "Gateway",
    //"port": "7002",

  },
  "AllowedHosts": "*"
}
